<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph.xml"
    app:startDestination="@id/contactListFragment">

    <fragment
        android:id="@+id/contactListFragment"
        android:name="com.software.reportapp.view.contact.list.ContactListFragment"
        android:label="fragment_contact_list"
        tools:layout="@layout/fragment_contact_list" >
        <action
            android:id="@+id/action_contactListFragment_to_addContactFragment2"
            app:destination="@id/addContactFragment" />
        <action
            android:id="@+id/action_contactListFragment_to_contactDetailFragment2"
            app:destination="@id/contactDetailFragment" />
    </fragment>

    <fragment
        android:id="@+id/addContactFragment"
        android:name="com.software.reportapp.view.contact.add.AddContactFragment"
        android:label="AddContactFragment" />
    <fragment
        android:id="@+id/contactDetailFragment"
        android:name="com.software.reportapp.view.contact.detail.ContactDetailFragment"
        android:label="ContactDetailFragment" />
</navigation>